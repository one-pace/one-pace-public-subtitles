Dialogue: 0,0:01:59.68,0:02:01.57,Main-207+,,0,0,0,,Ağabey!
Dialogue: 0,0:02:03.26,0:02:05.68,Main-207+,,0,0,0,,Holdem-{\i1}sama{\i0}!
Dialogue: 0,0:02:05.68,0:02:09.10,Main-207+,,0,0,0,,– Holdem-{\i1}sama{\i0}!\N– Holdem-{\i1}sama{\i0}!
Dialogue: 0,0:02:30.37,0:02:31.72,Main-207+,,0,0,0,,Tama!
Dialogue: 0,0:02:35.34,0:02:37.72,Main-207+,,0,0,0,,Ağabey!
Dialogue: 0,0:02:38.26,0:02:40.73,Main-207+,,0,0,0,,Sıkı tutun bakayım!
Dialogue: 0,0:02:40.73,0:02:41.89,Main-207+,,0,0,0,,Tamam!
Dialogue: 0,0:02:46.93,0:02:49.35,Main-207+,,0,0,0,,Pekâlâ! Tüyelim!
Dialogue: 0,0:02:58.73,0:03:00.53,Main-207+,,0,0,0,,Holdem-{\i1}sama{\i0}!
Dialogue: 0,0:03:00.53,0:03:02.15,Secondary-207+,,0,0,0,,Holdem-{\i1}sama{\i0}!
Dialogue: 0,0:03:05.33,0:03:09.61,Main-207+,,0,0,0,,Öncü biri olarak tek yumrukta mı bayıltıldın?
Dialogue: 0,0:03:10.29,0:03:11.96,Main-207+,,0,0,0,,Kim yaptı bunu?
Dialogue: 0,0:03:17.93,0:03:20.80,Main-207+,,0,0,0,,Ulan Luffy!\NYine yaptın yapacağını.
Dialogue: 0,0:03:20.80,0:03:22.59,Main-207+,,0,0,0,,Luffytaro-{\i1}san{\i0}!
Dialogue: 0,0:03:22.59,0:03:24.25,Main-207+,,0,0,0,,Otama-{\i1}chan{\i0}!
Dialogue: 0,0:03:24.25,0:03:28.27,Main-207+,,0,0,0,,Telaşlanma!\NVeledi Luffytaro'ya bırak!
Dialogue: 0,0:03:28.27,0:03:31.56,Main-207+,,0,0,0,,Durayım deme!\NKasabanın girişine tam gaz devam!
Dialogue: 0,0:03:37.11,0:03:42.48,Main-207+,,0,0,0,,Hazine gemisini geri alın!\NYoksa erzağımızdan olacağız!
Dialogue: 0,0:03:48.99,0:03:51.16,Main-207+,,0,0,0,,Sıkı tutun ha!
Dialogue: 0,0:03:51.16,0:03:54.17,Main-207+,,0,0,0,,Tutunuyorum ağabey!\NAsla bırakmam!
Dialogue: 0,0:03:59.60,0:04:01.76,Main-207+,,0,0,0,,A-Ağabey...
Dialogue: 0,0:04:01.76,0:04:03.68,Main-207+,,0,0,0,,Kılıçtan geçirin!
Dialogue: 0,0:04:33.46,0:04:36.13,Main-207+,,0,0,0,,Uza!
Dialogue: 0,0:04:45.89,0:04:50.46,Main-207+,,0,0,0,,Zorolar'a bir an önce yetişmezsem\Nbana yiyecek zırnık bırakmayacaklar!
Dialogue: 0,0:04:54.02,0:04:56.65,Main-207+,,0,0,0,,Ne güzel! Binelim! Atlıyorum!
Dialogue: 0,0:04:56.65,0:04:57.10,Main-207+,,0,0,0,,Ne?
Dialogue: 0,0:05:00.27,0:05:01.73,Main-207+,,0,0,0,,Beni...
Dialogue: 0,0:05:01.73,0:05:05.22,Main-207+,,0,0,0,,kollarıyla sarmalayan da kim böyle?
Dialogue: 0,0:05:05.22,0:05:07.41,Main-207+,,0,0,0,,Deh, at!
Dialogue: 0,0:05:07.41,0:05:08.74,Main-207+,,0,0,0,,Dur, n'apıyorsun be?
Dialogue: 0,0:05:08.74,0:05:11.49,Main-207+,,0,0,0,,– Ne? İnsan mıymış?\N– Ne? İnsan mıymış?
Comment: 0,0:05:11.49,0:05:13.45,Main-207+,,0,0,0,,İhihi!
Dialogue: 0,0:05:13.45,0:05:14.75,Main-207+,,0,0,0,,Yok be, at işte!
Dialogue: 0,0:05:14.75,0:05:17.08,Main-207+,,0,0,0,,Ne cüretle Speed-{\i1}sama{\i0}'nın\Nsırtına binersiniz?
Dialogue: 0,0:05:17.08,0:05:18.42,Main-207+,,0,0,0,,Alaşağı edin!
Dialogue: 0,0:05:18.42,0:05:22.01,Main-207+,,0,0,0,,İnsenize, edepsizler!
Dialogue: 0,0:05:24.27,0:05:26.58,Main-207+,,0,0,0,,Hey Tama, dango yap!
Dialogue: 0,0:05:26.58,0:05:27.26,Main-207+,,0,0,0,,Ne?
Dialogue: 0,0:05:27.26,0:05:29.35,Main-207+,,0,0,0,,Dediğimi yap!
Dialogue: 0,0:05:29.35,0:05:31.89,Main-207+,,0,0,0,,{\fad(150,150)}Kibi Dango!
Dialogue: 0,0:05:31.89,0:05:33.22,Main-207+,,0,0,0,,Şimdi, ata ver!
Dialogue: 0,0:05:33.22,0:05:37.08,Main-207+,,0,0,0,,Ne? İmkânsız!\Nİnsanlar üzerinde işe yaramaz!
Dialogue: 0,0:05:37.08,0:05:39.40,Main-207+,,0,0,0,,Denemeden bilemezsin!
Comment: 0,0:05:40.40,0:05:42.36,Main-207+,,0,0,0,,İhihi!
Dialogue: 0,0:05:42.36,0:05:44.43,Main-207+,,0,0,0,,Enfes duruyor!
Comment: 0,0:05:54.24,0:05:55.54,Main-207+,,0,0,0,,İhihi!
Dialogue: 0,0:05:55.54,0:06:00.20,Main-207+,,0,0,0,,Nereye gitmek istiyorsunuz, efendim?
Dialogue: 0,0:06:00.20,0:06:01.87,Main-207+,,0,0,0,,Yaşasın!
Dialogue: 0,0:06:01.87,0:06:03.71,Main-207+,,0,0,0,,İşe yaradı!
Dialogue: 0,0:06:04.42,0:06:06.20,Main-207+,,0,0,0,,Ama... nasıl?
Dialogue: 0,0:06:06.20,0:06:08.43,Main-207+,,0,0,0,,Yarı at olduğundan galiba.
Dialogue: 0,0:06:10.84,0:06:13.49,Main-207+,,0,0,0,,S-Speed-{\i1}sama{\i0}...
Dialogue: 0,0:06:13.49,0:06:15.35,Main-207+,,0,0,0,,Peşine düşün!
Dialogue: 0,0:06:18.87,0:06:19.90,Main-207+,,0,0,0,,Çok hızlı!
Dialogue: 0,0:06:19.90,0:06:23.42,Main-207+,,0,0,0,,Pekâlâ Kısrak!\NHazine gemisini takip et!
Dialogue: 0,0:06:23.42,0:06:25.69,Main-207+,,0,0,0,,Senin hizmetçin değilim!
Dialogue: 0,0:06:25.69,0:06:29.95,Main-207+,,0,0,0,,Adım Speed!\NArkadaşmışız gibi Kısrak deme!
Dialogue: 0,0:06:29.95,0:06:34.16,Main-207+,,0,0,0,,Ağabeyin lafını dinlemeni\Nistiyorum, Kısrak-{\i1}chan{\i0}.
Dialogue: 0,0:06:36.41,0:06:40.37,Main-207+,,0,0,0,,Başüstüne, efendim!
Dialogue: 0,0:06:40.37,0:06:44.82,Main-207+,,0,0,0,,Öncü Speed-{\i1}sama{\i0}'ya böyle\Nsöz geçirdiklerine inanamıyorum!
Dialogue: 0,0:06:44.82,0:06:47.26,Main-207+,,0,0,0,,Ona bir şeyle şantaj mı yapıyorlar ki?
Dialogue: 0,0:06:52.18,0:06:54.18,Thoughts-207+,,0,0,0,,Ağabey...
Dialogue: 0,0:06:55.25,0:06:58.23,Main-207+,,0,0,0,,{\fad(150,150)}Kızıl Şahin!
Dialogue: 0,0:06:59.44,0:07:01.52,Main-207+,,0,0,0,,{\fad(150,150)}Ateş Yumruk!
Dialogue: 0,0:07:08.89,0:07:12.28,Main-207+,,0,0,0,,Ağabey, demin aynı...
Dialogue: 0,0:07:14.71,0:07:16.99,Main-207+,,0,0,0,,Ace gibiydin.
Dialogue: 0,0:07:22.25,0:07:24.38,Main-207+,,0,0,0,,Ha? Ne?
Dialogue: 0,0:07:24.38,0:07:25.99,Main-207+,,0,0,0,,Bir şey mi dedin?
Dialogue: 0,0:07:30.80,0:07:31.92,Main-207+,,0,0,0,,Yok!
Dialogue: 0,0:07:32.80,0:07:34.60,Main-207+,,0,0,0,,Önemli değil.
Dialogue: 0,0:07:45.58,0:07:47.40,Main-207+,,0,0,0,,Kelleni alacağım.
Dialogue: 0,0:07:47.40,0:07:49.15,Main-207+,,0,0,0,,Ne dedin lan sen?
Dialogue: 0,0:07:51.32,0:07:52.88,Main-207+,,0,0,0,,Gebertelim şunu!
Dialogue: 0,0:07:52.88,0:07:53.74,Main-207+,,0,0,0,,– Aynen!\N– Aynen!
Dialogue: 0,0:07:53.74,0:07:55.24,Main-207+,,0,0,0,,{\fad(150,150)}Oda!
Dialogue: 0,0:07:57.78,0:08:00.00,Main-207+,,0,0,0,,Bu yetenek...
Dialogue: 0,0:08:00.65,0:08:02.21,Main-207+,,0,0,0,,Kolundaki dövme...
Dialogue: 0,0:08:03.00,0:08:03.96,Main-207+,,0,0,0,,Yoksa...
Dialogue: 0,0:08:09.49,0:08:10.95,Main-207+,,0,0,0,,Mümkün!
Dialogue: 0,0:08:11.34,0:08:14.01,Main-207+,,0,0,0,,İttifak kurmuşlar!
Dialogue: 0,0:08:18.85,0:08:23.35,Thoughts-207+,,0,0,0,,Planımızdan haberdar olan {\i1}Roronoa-{\i0}ya{\i1}\Nbizimkilerin yanında.
Dialogue: 0,0:08:24.18,0:08:29.30,Thoughts-207+,,0,0,0,,Ne kadar düşüncesiz hareket etseler de\NHoldem'i alt etmek gibi bir aptallık yapmazlar.
Dialogue: 0,0:08:30.73,0:08:33.36,Main-207+,,0,0,0,,Holdem-{\i1}sama{\i0}!
Dialogue: 0,0:08:33.36,0:08:37.57,Main-207+,,0,0,0,,Olamaz! Derhâl Jack-{\i1}sama{\i0}'ya\Nbildirmemiz gerek!
Dialogue: 0,0:08:37.57,0:08:39.45,Thoughts-207+,,0,0,0,,Ona el sürerlerse...
Dialogue: 0,0:08:39.45,0:08:42.01,Thoughts-207+,,0,0,0,,planımız...
Dialogue: 0,0:08:42.60,0:08:43.54,Thoughts-207+,,0,0,0,,suya düşer.
Dialogue: 0,0:08:47.13,0:08:49.81,Main-207+,,0,0,0,,Geber Hawkins!
Dialogue: 0,0:08:52.80,0:08:53.80,Main-207+,,0,0,0,,{\fad(150,150)}Karıştır!
Dialogue: 0,0:09:12.47,0:09:14.36,Main-207+,,0,0,0,,İmdat!
Dialogue: 0,0:09:14.36,0:09:17.74,Main-207+,,0,0,0,,Ne? Yaşıyor muyum?
Dialogue: 0,0:09:17.74,0:09:21.99,Main-207+,,0,0,0,,N'oluyor lan?\NÜst bedeni olmadan hareket ediyor!
Dialogue: 0,0:09:24.96,0:09:28.29,Thoughts-207+,,0,0,0,,Bir bakalım...\NNasıl bir yetenek kullandı?
Dialogue: 0,0:09:29.13,0:09:30.97,Main-207+,,0,0,0,,{\fad(150,150)}Hasır Kılıç!
Dialogue: 0,0:09:32.21,0:09:33.72,Main-207+,,0,0,0,,Maskeni çıkar,
Dialogue: 0,0:09:34.30,0:09:36.61,Main-207+,,0,0,0,,Trafalgar Law!
Dialogue: 0,0:09:57.57,0:10:02.45,Main-207+,,0,0,0,,Bu kadar çabuk ölmeyeceğini\Nbiliyordum, Basil Hawkins!
Dialogue: 0,0:10:03.18,0:10:03.91,Main-207+,,0,0,0,,Fakat...
Dialogue: 0,0:10:06.20,0:10:09.92,Main-207+,,0,0,0,,Gevzelik etmeyi kes!\NÇeneni kapamasını bilirim!
Dialogue: 0,0:10:09.92,0:10:11.88,Main-207+,,0,0,0,,Saldırın!
Dialogue: 0,0:10:11.88,0:10:12.59,Main-207+,,0,0,0,,Bekleyin!
Dialogue: 0,0:10:19.60,0:10:21.09,Main-207+,,0,0,0,,{\fad(150,150)}Kesme!
Dialogue: 0,0:10:27.39,0:10:28.35,Main-207+,,0,0,0,,{\fad(150,150)}Takt!
Dialogue: 0,0:10:37.74,0:10:38.86,Main-207+,,0,0,0,,{\fad(150,150)}Karıştır!
Dialogue: 0,0:10:59.58,0:11:01.80,Main-207+,,0,0,0,,Yedek canlarımın hepsini kestiğine göre...
Dialogue: 0,0:11:01.80,0:11:04.47,Main-207+,,0,0,0,,yeteneğimi çabucak çözmüşsün.
Dialogue: 0,0:11:04.47,0:11:06.01,Main-207+,,0,0,0,,Sadece seni zarflıyordum.
Dialogue: 0,0:11:06.78,0:11:10.27,Main-207+,,0,0,0,,Can almaktan hoşlanmıyorum,\Nne de olsa bir doktorum.
Dialogue: 0,0:11:11.71,0:11:16.32,Main-207+,,0,0,0,,Wano'ya hoş geldin. Sabaody'deki o günden bu yana görüşmemiştik.
Dialogue: 0,0:11:16.32,0:11:21.28,Main-207+,,0,0,0,,Müttefiklerin de burada olduğuna göre\Ntatile gelmediğini varsayıyorum.
Dialogue: 0,0:11:22.62,0:11:24.70,Main-207+,,0,0,0,,Fark etmeyeceğimi mi sanmıştın?
Dialogue: 0,0:11:24.70,0:11:27.66,Main-207+,,0,0,0,,Bu sabah\NHasır Şapkalar'la karşılaştım.
Dialogue: 0,0:11:29.31,0:11:33.12,Main-207+,,0,0,0,,Planınız açığa çıkmadan\Nbeni susturmak istemiş olmalısın.
Dialogue: 0,0:11:38.89,0:11:43.22,Main-207+,,0,0,0,,{\i1}Soyguncular Holdem-sama'yı alt ettiler, şu an kaçıyorlar!
Dialogue: 0,0:11:43.22,0:11:45.30,Main-207+,,0,0,0,,{\i1}Durumu ise Jack-sama'ya bildirdik!
Dialogue: 0,0:11:45.30,0:11:49.77,Thoughts-207+,,0,0,0,,Holdem'i devirdikleri yetmezmiş gibi\NJack da haberdar mı edilmiş?
Dialogue: 0,0:11:50.06,0:11:55.08,Main-207+,,0,0,0,,İşittiğin üzere\Nboşa kürek çekmiş oldun.
Dialogue: 0,0:11:56.13,0:12:02.40,Main-207+,,0,0,0,,Tabii Hasır Şapkalar'la olan iş birliğiniz\NKaido-{\i1}san{\i0}'ın da kulağına gidecek.
Dialogue: 0,0:12:04.65,0:12:09.63,Thoughts-207+,,0,0,0,,Sersem herifler! Bizi olabilecek\Nen kötü duruma soktular!
Dialogue: 0,0:12:13.96,0:12:18.02,Main-207+,,0,0,0,,Lan yoldakiler! Çekilin!
Dialogue: 0,0:12:18.02,0:12:19.46,Main-207+,,0,0,0,,Roronoa-{\i1}ya{\i0}?!
Dialogue: 0,0:12:19.46,0:12:23.35,Main-207+,,0,0,0,,Affedersiniz! Açılın lütfen!\NAcelemiz var da!
Dialogue: 0,0:12:33.18,0:12:36.44,Main-207+,,0,0,0,,Peşlerine düşün! Erzağımızı çaldılar!
Dialogue: 0,0:12:43.11,0:12:45.09,Main-207+,,0,0,0,,Roronoa-{\i1}ya{\i0}!
Dialogue: 0,0:12:45.09,0:12:46.78,Main-207+,,0,0,0,,N'aber Trao?
Dialogue: 0,0:12:48.03,0:12:49.20,Main-207+,,0,0,0,,Yapacağın işi...
Dialogue: 0,0:13:05.59,0:13:07.26,Main-207+,,0,0,0,,Derdin ne, Trao?
Dialogue: 0,0:13:07.26,0:13:09.10,Main-207+,,0,0,0,,Lanet herif...
Dialogue: 0,0:13:09.10,0:13:11.47,Main-207+,,0,0,0,,Ne? Bu da kim?
Dialogue: 0,0:13:13.02,0:13:17.35,Main-207+,,0,0,0,,Aranan biri olman yetmezmiş gibi\NJack'in da dikkatini çekecek bir halt yedin!
Dialogue: 0,0:13:17.35,0:13:21.35,Main-207+,,0,0,0,,Olay çıkarmayacağına söz vermiştin!
Dialogue: 0,0:13:22.22,0:13:23.57,Main-207+,,0,0,0,,Üzgünüm.
Dialogue: 0,0:13:23.57,0:13:25.36,Main-207+,,0,0,0,,Hem...
Dialogue: 0,0:13:25.36,0:13:27.45,Main-207+,,0,0,0,,Kuri'de ne işin var ki?
Dialogue: 0,0:13:27.45,0:13:31.41,Main-207+,,0,0,0,,Çiçek Başkenti'nde bir {\i1}ronin{\i0} olman gerekiyordu!
Dialogue: 0,0:13:32.31,0:13:33.54,Main-207+,,0,0,0,,Tanıdık mı?
Dialogue: 0,0:13:33.54,0:13:35.52,Main-207+,,0,0,0,,Her şey yolunda mı?
Dialogue: 0,0:13:35.52,0:13:36.79,Main-207+,,0,0,0,,Nah yolunda!
Dialogue: 0,0:13:36.79,0:13:38.26,Main-207+,,0,0,0,,Pardon.
Dialogue: 0,0:13:38.63,0:13:41.24,Main-207+,,0,0,0,,El mâhkumdu.
Dialogue: 0,0:13:41.24,0:13:43.55,Main-207+,,0,0,0,,Luffy'yle karşılaştım da.
Dialogue: 0,0:13:45.17,0:13:48.89,Main-207+,,0,0,0,,Hasır Şapka-{\i1}ya{\i0}!\NYine altından o çıktı!
Dialogue: 0,0:13:58.69,0:14:01.06,Main-207+,,0,0,0,,Zorojuro-{\i1}san{\i0}! Kasabadan çıkıyoruz!
Dialogue: 0,0:14:01.91,0:14:03.55,Main-207+,,0,0,0,,Güzel.
Dialogue: 0,0:14:03.55,0:14:05.78,Main-207+,,0,0,0,,Okobore Kasabası'nda erzağı indirin!
Dialogue: 0,0:14:07.78,0:14:11.00,Main-207+,,0,0,0,,Anne ya! Karnım kazınıyor!
Dialogue: 0,0:14:11.00,0:14:12.74,Main-207+,,0,0,0,,Biraz daha dayan oğlum.
Dialogue: 0,0:14:19.02,0:14:20.33,Main-207+,,0,0,0,,Ne oluyor?
Dialogue: 0,0:14:30.47,0:14:32.22,Main-207+,,0,0,0,,B-Bu...
Dialogue: 0,0:14:32.22,0:14:35.32,Main-207+,,0,0,0,,hazine gemisi!
Dialogue: 0,0:14:35.64,0:14:38.52,Main-207+,,0,0,0,,Bir sürü balık var!
Dialogue: 0,0:14:38.52,0:14:41.12,Main-207+,,0,0,0,,Yiyecek! Yiyecek!
Dialogue: 0,0:14:56.33,0:14:58.04,Main-207+,,0,0,0,,Hazine gemisi geldi!
Dialogue: 0,0:14:58.04,0:15:01.37,Main-207+,,0,0,0,,Kurtulduk!
Dialogue: 0,0:15:03.79,0:15:05.59,Main-207+,,0,0,0,,Süper!
Dialogue: 0,0:15:05.59,0:15:07.88,Main-207+,,0,0,0,,Herkes acele etsin!
Dialogue: 0,0:15:39.65,0:15:41.54,Main-207+,,0,0,0,,Yiyecek!
Dialogue: 0,0:15:41.54,0:15:43.40,Main-207+,,0,0,0,,Yiyebilir miyiz?
Dialogue: 0,0:15:43.40,0:15:44.95,Main-207+,,0,0,0,,Şükürler olsun!
Dialogue: 0,0:15:44.95,0:15:46.34,Main-207+,,0,0,0,,Hayatta kalabileceğiz!
Dialogue: 0,0:15:47.01,0:15:49.30,Main-207+,,0,0,0,,Enfes!
Dialogue: 0,0:15:49.93,0:15:53.26,Main-207+,,0,0,0,,Gemi dolusu erzak!
Dialogue: 0,0:15:53.91,0:15:57.22,Main-207+,,0,0,0,,Artık karnımız kazınmayacak, teşekkürler!
Dialogue: 0,0:15:58.05,0:16:00.06,Main-207+,,0,0,0,,Şükürler olsun.
Dialogue: 0,0:16:07.40,0:16:10.03,Main-207+,,0,0,0,,Hayır lan, bekleyin!
Dialogue: 0,0:16:10.03,0:16:12.74,Main-207+,,0,0,0,,Gemidekiler artık değil!
Dialogue: 0,0:16:12.74,0:16:15.62,Main-207+,,0,0,0,,Hazine gemisinden uzaklaşın!
Dialogue: 0,0:16:16.62,0:16:18.99,Main-207+,,0,0,0,,Avam müsveddeleri!
Dialogue: 0,0:16:18.99,0:16:23.87,Main-207+,,0,0,0,,Erzağa elinizi dahi sürmeyin!
Dialogue: 0,0:16:23.87,0:16:26.75,Main-207+,,0,0,0,,Haddini bilmeyen budalalar!
Dialogue: 0,0:16:27.54,0:16:29.55,Main-207+,,0,0,0,,Buradan ileri geçemezsiniz!
Dialogue: 0,0:16:29.55,0:16:31.84,Main-207+,,0,0,0,,Avamlardan taraf oluyorsunuz demek!
Dialogue: 0,0:16:31.84,0:16:35.22,Main-207+,,0,0,0,,Sıçtıklarım, bunun bedelini\Nağır ödeyeceksiniz!
Dialogue: 0,0:16:39.26,0:16:41.27,Main-207+,,0,0,0,,Hey!
Dialogue: 0,0:16:42.18,0:16:44.90,Main-207+,,0,0,0,,Beklettiğim için üzgünüm!
Dialogue: 0,0:16:55.32,0:16:57.66,Main-207+,,0,0,0,,Su getirdim!
Dialogue: 0,0:17:00.04,0:17:03.91,Main-207+,,0,0,0,,Zehirli de değil! Kana kana için!
Dialogue: 0,0:17:15.67,0:17:19.26,Main-207+,,0,0,0,,Yıllardır bu kadar\Niçme suyumuz olmamıştı!
Dialogue: 0,0:17:19.26,0:17:21.93,Main-207+,,0,0,0,,Şükürler olsun! Şükürler olsun!
Dialogue: 0,0:17:23.85,0:17:27.98,Main-207+,,0,0,0,,Adım Luffytaro! Soran olursa söylersiniz!
Dialogue: 0,0:17:37.02,0:17:39.82,Main-207+,,0,0,0,,Luffytaro-{\i1}san{\i0}, teşekkürler!
Dialogue: 0,0:17:39.82,0:17:42.20,Main-207+,,0,0,0,,Ağabey!
Dialogue: 0,0:17:43.22,0:17:44.16,Main-207+,,0,0,0,,Tama!
Dialogue: 0,0:17:45.37,0:17:46.66,Main-207+,,0,0,0,,Amanın.
Dialogue: 0,0:17:46.66,0:17:48.08,Main-207+,,0,0,0,,Trao!
Dialogue: 0,0:17:48.08,0:17:50.29,Main-207+,,0,0,0,,Uzun zaman oldu!
Dialogue: 0,0:17:50.29,0:17:52.05,Main-207+,,0,0,0,,Hasır Şapka-{\i1}ya{\i0}...
Dialogue: 0,0:17:52.05,0:17:55.76,Main-207+,,0,0,0,,Wano'da isyan çıkardın.
Dialogue: 0,0:17:55.76,0:17:58.99,Main-207+,,0,0,0,,İsyan mı? Yok be!
Dialogue: 0,0:18:00.91,0:18:01.60,Main-207+,,0,0,0,,Al bakalım!
Dialogue: 0,0:18:07.22,0:18:08.90,Main-207+,,0,0,0,,Elma!
Dialogue: 0,0:18:09.65,0:18:10.97,Main-207+,,0,0,0,,Tama...
Dialogue: 0,0:18:14.18,0:18:18.78,Main-207+,,0,0,0,,Bana yaptığın yemeğin\Nkarşılığını veriyorum.
Dialogue: 0,0:18:18.78,0:18:21.15,Main-207+,,0,0,0,,Daha yeni başladım!
Dialogue: 0,0:18:30.63,0:18:32.71,Main-207+,,0,0,0,,Şu ete bir bakın ya!
Dialogue: 0,0:18:34.53,0:18:36.63,Main-207+,,0,0,0,,Hey! Siz!
Dialogue: 0,0:18:36.63,0:18:38.63,Main-207+,,0,0,0,,Bana da et bırakın oğlum!
Dialogue: 0,0:18:38.63,0:18:41.73,Main-207+,,0,0,0,,Et! Et, et!
Dialogue: 0,0:18:41.73,0:18:43.56,Main-207+,,0,0,0,,Et, et, et, et!
Dialogue: 0,0:18:43.56,0:18:45.17,Main-207+,,0,0,0,,Et!
Dialogue: 0,0:18:56.48,0:18:58.28,Main-207+,,0,0,0,,Holdem-{\i1}sama{\i0}!
Dialogue: 0,0:18:58.28,0:19:00.45,Main-207+,,0,0,0,,Yangın yayılıyor!
Dialogue: 0,0:19:00.45,0:19:02.68,Main-207+,,0,0,0,,İtfaiyeciler nerede?
Dialogue: 0,0:19:04.74,0:19:07.67,Main-207+,,0,0,0,,Holdem-{\i1}sama{\i0}! Dayanın!
Dialogue: 0,0:19:07.67,0:19:10.12,Main-207+,,0,0,0,,Gözlerinizi açın lütfen!
Dialogue: 0,0:19:17.18,0:19:20.18,Main-207+,,0,0,0,,Yalnızca aslan uyandı! Korkunç!
Dialogue: 0,0:19:23.60,0:19:25.35,Main-207+,,0,0,0,,Evet.
Dialogue: 0,0:19:25.35,0:19:28.73,Main-207+,,0,0,0,,Monkey D. Luffy, Roronoa Zoro...
Dialogue: 0,0:19:28.73,0:19:30.94,Main-207+,,0,0,0,,ve Trafalgar Law.
Dialogue: 0,0:19:31.60,0:19:33.65,Main-207+,,0,0,0,,Kendimi aciz göstermek istemesem de...
Dialogue: 0,0:19:34.51,0:19:39.61,Main-207+,,0,0,0,,tehlikeli üç korsanla ben bile\Nyalnız başa çıkamam.
Dialogue: 0,0:19:57.96,0:20:00.95,Main-207+,,0,0,0,,Enfes ya!
Dialogue: 0,0:20:01.69,0:20:03.64,Main-207+,,0,0,0,,Ne mutlu sana, Tama!
Dialogue: 0,0:20:04.33,0:20:05.47,Main-207+,,0,0,0,,E-Evet!
Dialogue: 0,0:20:05.47,0:20:09.37,Main-207+,,0,0,0,,Bundan daha iyi bir doğum günü\Ngeçireceğimi sanmıyorum!
Dialogue: 0,0:20:09.37,0:20:10.68,Main-207+,,0,0,0,,Tama!
Dialogue: 0,0:20:10.68,0:20:13.10,Main-207+,,0,0,0,,Küçük düşünme!
Dialogue: 0,0:20:13.10,0:20:15.48,Main-207+,,0,0,0,,Her gününü böyle geçirmeni sağlayacağım!
Dialogue: 0,0:20:17.44,0:20:20.44,Main-207+,,0,0,0,,Ülkeden ayrılacağım güne dek,
Dialogue: 0,0:20:20.44,0:20:24.28,Main-207+,,0,0,0,,burayı tıka basa karnını doyurabileceğin bir yere çevireceğim!
Dialogue: 0,0:20:47.71,0:20:51.10,Main-207+,,0,0,0,,Bu ülkeye tekrar geldiğimizde...
Dialogue: 0,0:20:51.10,0:20:55.31,Main-207+,,0,0,0,,tıka basa karnını doyurabileceğin\Nbir yere çevireceğim!
Dialogue: 0,0:21:15.58,0:21:17.00,Main-207+,,0,0,0,,Ha, hoşuna gitmedi mi?
Dialogue: 0,0:21:18.05,0:21:19.00,Main-207+,,0,0,0,,Ne?
Dialogue: 0,0:21:19.08,0:21:20.96,Main-207+,,0,0,0,,N-N-Ne?!
Dialogue: 0,0:21:20.96,0:21:23.13,Main-207+,,0,0,0,,E-Elbette...
Dialogue: 0,0:21:23.13,0:21:26.12,Main-207+,,0,0,0,,Elbette mutluyum ya!
Dialogue: 0,0:21:28.69,0:21:30.76,Main-207+,,0,0,0,,Biraz şaşırdım sadece.
Dialogue: 0,0:21:34.49,0:21:35.46,Main-207+,,0,0,0,,Anladım!
Dialogue: 0,0:21:39.36,0:21:42.90,Main-207+,,0,0,0,,Ulan, çok düşüncesizce hareket ettin!
Dialogue: 0,0:21:42.90,0:21:46.99,Main-207+,,0,0,0,,Şimdi, gerçekten peşimizden gelecekler!\NHazır ol!
Dialogue: 0,0:21:46.99,0:21:48.66,Main-207+,,0,0,0,,Sen hiç konuşma lan!
Dialogue: 0,0:21:49.16,0:21:52.74,Main-207+,,0,0,0,,Burada uzun süre kalmamalıyız,\Nyola koyulalım!
Dialogue: 0,0:21:53.98,0:21:55.19,Main-207+,,0,0,0,,Peşime düşün.
Dialogue: 0,0:21:56.73,0:22:00.92,Main-207+,,0,0,0,,Dağın zirvesindeki\NOden Kalesi'nin kalıntılarına gidiyoruz!
Dialogue: 0,0:22:03.88,0:22:06.05,Main-207+,,0,0,0,,Öyleyse ben de gidiyorum.
Dialogue: 0,0:22:08.60,0:22:10.65,Main-207+,,0,0,0,,Teşekkür ederim.
Dialogue: 0,0:22:10.65,0:22:13.93,Main-207+,,0,0,0,,Pekâlâ, ona gözün gibi bak, Kısrak!
Dialogue: 0,0:22:13.93,0:22:18.82,Main-207+,,0,0,0,,Canavar Korsanları'ndan\Nöncü Speed'im ben!
Dialogue: 0,0:22:18.82,0:22:21.44,Main-207+,,0,0,0,,Sersemin tekinden emir almam!
Dialogue: 0,0:22:22.73,0:22:25.82,Main-207+,,0,0,0,,Efendisini korumak\Nhizmetkârın boynunun borcudur zaten!
Dialogue: 0,0:22:25.82,0:22:29.86,Thoughts-207+,,0,0,0,,Öncü niye veledin lafına bakıyor ki?
Dialogue: 0,0:22:31.11,0:22:32.20,Main-207+,,0,0,0,,Ağabey...
Dialogue: 0,0:22:32.20,0:22:34.10,Main-207+,,0,0,0,,Ha? Ne oldu?
Dialogue: 0,0:22:35.22,0:22:38.21,Main-207+,,0,0,0,,Hiç unutmayacağım...
Dialogue: 0,0:22:43.13,0:22:50.34,Main-207+,,0,0,0,,Bugün içtiğim çorbayla elmanın tadını\Nhayatım boyunca unutmayacağım!
Dialogue: 0,0:22:50.34,0:22:51.59,Main-207+,,0,0,0,,Süper!
Dialogue: 0,0:22:51.59,0:22:56.97,Main-207+,,0,0,0,,Amigasa Köyü'ne dönünce\Nelmayı da muhterem üstadıma vereceğim!
Dialogue: 0,0:22:56.97,0:23:01.27,Main-207+,,0,0,0,,Bunak Burna da selam söyle!\NKöpeği getireceğime söz veriyorum!
Dialogue: 0,0:23:01.27,0:23:02.74,Main-207+,,0,0,0,,Tamam!
Dialogue: 0,0:23:02.74,0:23:03.86,Main-207+,,0,0,0,,Komachiyo!
Dialogue: 0,0:23:03.86,0:23:07.01,Main-207+,,0,0,0,,Ağabeye yardım etmeni istiyorum.
Dialogue: 0,0:23:08.36,0:23:09.82,Main-207+,,0,0,0,,Sahi!
Dialogue: 0,0:23:09.82,0:23:15.18,Main-207+,,0,0,0,,Kılıç biraz daha bende kalsın!\NKılıçsız samuray olamam!
Dialogue: 0,0:23:15.18,0:23:18.50,Main-207+,,0,0,0,,Kullandığın yok ki!
Dialogue: 0,0:23:18.50,0:23:22.08,Main-207+,,0,0,0,,Ver de bir bakayım yav, hayret bir şeysin!
Dialogue: 0,0:23:22.46,0:23:25.46,Main-207+,,0,0,0,,Muhterem üstadıma iletirim!
Dialogue: 0,0:23:25.46,0:23:27.33,Main-207+,,0,0,0,,Tamam, teşekkürler!
Dialogue: 0,0:23:27.33,0:23:30.01,Main-207+,,0,0,0,,Artık yola koyulalım efendim!
Dialogue: 0,0:23:30.01,0:23:32.25,Main-207+,,0,0,0,,Sana zahmet!
Dialogue: 0,0:23:32.25,0:23:34.55,Main-207+,,0,0,0,,Yolunuz açık olsun!
Dialogue: 0,0:23:34.55,0:23:36.01,Main-207+,,0,0,0,,Peki! Öyleyse...
Dialogue: 0,0:23:38.02,0:23:40.94,Main-207+,,0,0,0,,görüşürüz!
Dialogue: 0,0:23:44.98,0:23:47.07,Main-207+,,0,0,0,,Görüşürüz!
Dialogue: 0,0:23:47.07,0:23:49.81,Main-207+,,0,0,0,,Tama!
Dialogue: 0,0:23:58.98,0:24:00.12,Main-207+,,0,0,0,,Hadi bakalım...
Dialogue: 0,0:24:00.12,0:24:01.41,Main-207+,,0,0,0,,Luffy!
Dialogue: 0,0:24:03.47,0:24:04.75,Main-207+,,0,0,0,,Biz de yolumuza bakalım!
Dialogue: 0,0:24:05.25,0:24:06.92,Main-207+,,0,0,0,,Luffytaro-{\i1}san{\i0}!
Dialogue: 0,0:24:09.01,0:24:11.01,Main-207+,,0,0,0,,– Luffytaro-{\i1}san{\i0}!\N– Luffytaro-{\i1}san{\i0}!
Dialogue: 0,0:24:11.01,0:24:13.89,Main-207+,,0,0,0,,İlk kez böyle bir ziyafet görüyorum!
Dialogue: 0,0:24:13.89,0:24:16.00,Main-207+,,0,0,0,,Dedem de ayaklanıverdi!
Dialogue: 0,0:24:16.00,0:24:18.01,Main-207+,,0,0,0,,Artık gözüm açık gitmem!
Dialogue: 0,0:24:18.01,0:24:20.23,Main-207+,,0,0,0,,Evladımızın da yüzüne kan geldi!
Dialogue: 0,0:24:21.85,0:24:25.31,Main-207+,,0,0,0,,İlk kez karnım bu kadar doydu ya!
Dialogue: 0,0:24:35.27,0:24:36.61,Main-207+,,0,0,0,,Ne güzel!
Dialogue: 0,0:24:41.70,0:24:43.92,Main-207+,,0,0,0,,Tamam öyleyse, biz kaçtık!
Dialogue: 0,0:24:43.92,0:24:48.67,Main-207+,,0,0,0,,– Hazine gemisi için teşekkürler!\N– Hazine gemisi için teşekkürler!
Dialogue: 0,0:24:48.67,0:24:50.64,Main-207+,,0,0,0,,– Minnettarız!\N– Minnettarız!
Dialogue: 0,0:24:50.64,0:24:52.92,Main-207+,,0,0,0,,– Yine gelin!\N– Teşekkürler!
Dialogue: 0,0:24:53.71,0:24:56.63,Main-207+,,0,0,0,,Korsanlar iyilik mi yaparmış?
Dialogue: 0,0:24:56.63,0:24:57.64,Main-207+,,0,0,0,,Tiksindim!
Dialogue: 0,0:24:59.51,0:25:02.43,Main-207+,,0,0,0,,İyi bir iş yapmışız ha!
Dialogue: 0,0:25:04.76,0:25:06.55,Main-207+,,0,0,0,,Hırsızlık yaptın.
Dialogue: 0,0:25:10.36,0:25:12.19,Main-207+,,0,0,0,,Millet!
Dialogue: 0,0:25:12.19,0:25:15.11,Main-207+,,0,0,0,,Kendinize iyi bakın!
Dialogue: 0,0:25:24.07,0:25:27.81,Main-207+,,0,0,0,,Dango yediğin için onun\Nhizmetkârı olmadın yani.
Dialogue: 0,0:25:29.71,0:25:31.05,Main-207+,,0,0,0,,Peki...
Dialogue: 0,0:25:31.05,0:25:33.32,Main-207+,,0,0,0,,sen niye peşimize takılıyorsun?
Dialogue: 0,0:25:34.11,0:25:38.51,Main-207+,,0,0,0,,Ö-Özür dilerim! Oden Kalesi'ni\Nhep ziyaret etmek istemişimdir!
Dialogue: 0,0:25:38.51,0:25:43.41,Main-207+,,0,0,0,,Vardıktan sonra kendim dönebilirim,\Nsize yük olmam!
Dialogue: 0,0:25:44.98,0:25:46.50,Main-207+,,0,0,0,,Sorun olmasın?
Dialogue: 0,0:25:47.31,0:25:48.40,Main-207+,,0,0,0,,Merak etme!
Dialogue: 0,0:25:48.40,0:25:52.32,Main-207+,,0,0,0,,Kiku hem iyi kalpli hem de güçlüdür,\Nsorun olmaz!
Dialogue: 0,0:25:54.03,0:25:55.40,Main-207+,,0,0,0,,Hayır,
Dialogue: 0,0:25:55.40,0:25:57.97,Main-207+,,0,0,0,,bir şeyler saklıyor.
Dialogue: 0,0:25:58.98,0:26:02.29,Main-207+,,0,0,0,,Bu arada Trao, niye kaleye gidiyoruz?
Dialogue: 0,0:26:02.29,0:26:04.41,Main-207+,,0,0,0,,Eskiden kaleydi.
Dialogue: 0,0:26:05.07,0:26:07.81,Thoughts-207+,,0,0,0,,Terk edilmiş kalıntılar...
Dialogue: 0,0:26:07.81,0:26:10.99,Thoughts-207+,,0,0,0,,Oden Kalesi'nde ne işi var ki?
Dialogue: 0,0:26:11.54,0:26:14.34,Thoughts-207+,,0,0,0,,Luffytaro-{\i1}san{\i0}lar iyi insanlar olsa da...
Dialogue: 0,0:26:14.34,0:26:16.61,Thoughts-207+,,0,0,0,,henüz tedbiri elden bırakamam.
Dialogue: 0,0:26:18.59,0:26:21.03,Main-207+,,0,0,0,,Sizden ayrıldıktan sonra...
Dialogue: 0,0:26:21.03,0:26:25.05,Main-207+,,0,0,0,,bir plan yaparak o doğrultuda\Nhareket etmeye başladık.
Dialogue: 0,0:26:25.05,0:26:28.02,Main-207+,,0,0,0,,Neyse, varınca görürsün.
Dialogue: 0,0:26:28.02,0:26:29.19,Main-207+,,0,0,0,,Öncelikle...
Dialogue: 0,0:26:29.19,0:26:32.43,Main-207+,,0,0,0,,Wano'nun hayaletleriyle buluşacağız.
Dialogue: 0,0:26:34.11,0:26:35.23,Main-207+,,0,0,0,,Hayaletler mi?
Dialogue: 0,0:26:36.38,0:26:38.06,Main-207+,,0,0,0,,Küçük dilini yutacaksın.
Dialogue: 0,0:26:39.73,0:26:43.28,Main-207+,,0,0,0,,Evet, ben de afallamıştım.
Dialogue: 0,0:26:47.65,0:26:49.79,Main-207+,,0,0,0,,Orada...
Dialogue: 0,0:26:58.45,0:27:01.68,Main-207+,,0,0,0,,akıl almaz hakikati öğreneceksin!
Dialogue: 0,0:27:05.71,0:27:06.89,Main-207+,,0,0,0,,Her hâlükârda...
Dialogue: 0,0:27:06.89,0:27:11.73,Main-207+,,0,0,0,,konuşma faslını Oden Kalesi'nin kalıntılarının bulunduğu dağın zirvesine varınca yaparız.
Comment: 0,0:27:11.73,0:27:14.61,Main-207+,,0,0,0,,Aman be!
Dialogue: 0,0:27:13.40,0:27:14.61,Main-207+,,0,0,0,,Gıcık!
Dialogue: 0,0:27:24.78,0:27:26.37,Main-207+,,0,0,0,,Çetin bir ceviz var.
Dialogue: 0,0:27:26.37,0:27:27.70,Main-207+,,0,0,0,,Evet.
Dialogue: 0,0:27:28.29,0:27:30.25,Main-207+,,0,0,0,,Orochi'nin suikastçisi olabilir.
Dialogue: 0,0:27:30.25,0:27:32.00,Main-207+,,0,0,0,,Luffy, siz devam edin.
Dialogue: 0,0:27:32.91,0:27:36.21,Main-207+,,0,0,0,,İcabına bakar bakmaz size yetişirim.
Dialogue: 0,0:27:36.21,0:27:37.17,Main-207+,,0,0,0,,Elinden öper.
Dialogue: 0,0:27:42.72,0:27:45.61,Main-207+,,0,0,0,,Adamakıllı saklandığını mı sanıyorsun?
Dialogue: 0,0:28:03.41,0:28:05.24,Main-207+,,0,0,0,,Kale male yok!
Dialogue: 0,0:28:05.24,0:28:06.64,Main-207+,,0,0,0,,Kalıntı dedik ya.
Dialogue: 0,0:28:06.64,0:28:07.71,Main-207+,,0,0,0,,Sahi.
Dialogue: 0,0:28:07.71,0:28:09.59,Main-207+,,0,0,0,,Peşimden gel, bu taraftan.
Dialogue: 0,0:28:16.62,0:28:18.97,Main-207+,,0,0,0,,Ne? Hey, Kiku!
Dialogue: 0,0:28:19.67,0:28:21.47,Main-207+,,0,0,0,,Nesi var acaba?
Dialogue: 0,0:28:21.47,0:28:25.18,Main-207+,,0,0,0,,Sahi, Zoro da gelmedi daha.
Dialogue: 0,0:28:31.56,0:28:34.65,Main-207+,,0,0,0,,Ürpertici bir yermiş.
Dialogue: 0,0:28:34.65,0:28:37.40,Main-207+,,0,0,0,,20 yıldır ayak basan olmadı.
Dialogue: 0,0:28:41.93,0:28:43.17,Main-207+,,0,0,0,,Onlar nedir?
Dialogue: 0,0:28:47.78,0:28:50.21,Main-207+,,0,0,0,,"Kozuki Oden'in..."?
Dialogue: 0,0:28:52.54,0:28:55.25,Main-207+,,0,0,0,,Diğer mezar taşlarına da göz at.
Dialogue: 0,0:28:57.40,0:28:59.15,Main-207+,,0,0,0,,"Kozuki..."
Dialogue: 0,0:29:02.82,0:29:04.72,Main-207+,,0,0,0,,Momonosuke derken...
Dialogue: 0,0:29:05.72,0:29:09.05,Main-207+,,0,0,0,,Tanıdığın veledin mezarı.
Dialogue: 0,0:29:09.05,0:29:11.81,Main-207+,,0,0,0,,Ne? Neler zırvalıyorsun Trao?
Dialogue: 0,0:29:11.81,0:29:14.77,Main-207+,,0,0,0,,Yalnızca veletten ibaret de değil.
Dialogue: 0,0:29:18.36,0:29:19.95,Main-207+,,0,0,0,,"Raizo"?!
Dialogue: 0,0:29:21.82,0:29:23.55,Main-207+,,0,0,0,,"Kanjuro"?!
Dialogue: 0,0:29:25.15,0:29:26.92,Main-207+,,0,0,0,,"Kin'emon"?!
Dialogue: 0,0:29:30.12,0:29:32.79,Main-207+,,0,0,0,,Neden mezarları var ki?
Dialogue: 0,0:29:41.27,0:29:44.32,Main-207+,,0,0,0,,Dur biraz, Wano'ya girerken...
Dialogue: 0,0:29:44.32,0:29:47.30,Main-207+,,0,0,0,,Kin'emon'a ulaşamamıştım.
Dialogue: 0,0:29:54.91,0:29:57.12,Main-207+,,0,0,0,,Y- Yoksa...
Dialogue: 0,0:30:03.32,0:30:05.41,Main-207+,,0,0,0,,öldüler mi?
Dialogue: 0,0:30:17.33,0:30:19.35,Main-207+,,0,0,0,,Tamam, geri dönelim...
Dialogue: 0,0:30:22.60,0:30:25.72,Main-207+,,0,0,0,,Nereye gitti bunlar?
Dialogue: 0,0:30:30.71,0:30:32.35,Main-207+,,0,0,0,,Rıhtım.
Dialogue: 0,0:30:47.53,0:30:49.06,Main-207+,,0,0,0,,Mümkün değil!
Dialogue: 0,0:30:51.99,0:30:53.66,Main-207+,,0,0,0,,Ölmüş olamazlar!
Dialogue: 0,0:30:53.66,0:30:56.28,Main-207+,,0,0,0,,Kin'emon'un burada beklediğini sanmıştım.
Dialogue: 0,0:30:56.31,0:30:58.87,Main-207+,,0,0,0,,Uzun zaman önce aramızdan ayrıldı.
Dialogue: 0,0:30:58.87,0:30:59.87,Main-207+,,0,0,0,,Ayrıldı mı?
Dialogue: 0,0:30:59.87,0:31:02.33,Main-207+,,0,0,0,,Gece vakti uğrayabilir ama.
Dialogue: 0,0:31:02.33,0:31:04.99,Main-207+,,0,0,0,,Dün gece de defalarca belirdi.
Dialogue: 0,0:31:07.72,0:31:10.51,Main-207+,,0,0,0,,Ne hayaletmiş gibi konuşuyorsun be!
Dialogue: 0,0:31:10.51,0:31:14.93,Main-207+,,0,0,0,,Bu mezarlar ne alaka?\NÇıkar ağzındaki baklayı, Trao!
Dialogue: 0,0:31:14.93,0:31:17.14,Main-207+,,0,0,0,,Dillendirmek istemediğin bir şey mi var?
Dialogue: 0,0:31:18.07,0:31:20.81,Main-207+,,0,0,0,,Söylemek bana düşmez.
Dialogue: 0,0:31:53.40,0:31:55.22,Main-207+,,0,0,0,,Luffy-{\i1}dono{\i0}!
Dialogue: 0,0:31:56.65,0:31:57.14,Main-207+,,0,0,0,,Ne?
Dialogue: 0,0:31:59.06,0:32:00.06,Main-207+,,0,0,0,,Kin!
Dialogue: 0,0:32:00.06,0:32:02.73,Main-207+,,0,0,0,,Neyse ki hayattasın!
Dialogue: 0,0:32:02.73,0:32:06.21,Main-207+,,0,0,0,,Girişte karşılamaya\Ngelemediğim için üzgünüm.
Dialogue: 0,0:32:06.21,0:32:08.57,Main-207+,,0,0,0,,Sağ salim varabilmenize çok sevindim.
Dialogue: 0,0:32:08.57,0:32:12.00,Main-207+,,0,0,0,,Öyle cırcır olmuşum ki...
Dialogue: 0,0:32:12.00,0:32:16.46,Main-207+,,0,0,0,,bu geceyi de\Ntuvalatte geçiririm diyordum.
Dialogue: 0,0:32:16.98,0:32:20.37,Main-207+,,0,0,0,,{\an8}Ama ufaktan toparlamaya başladım.
Dialogue: 0,0:32:20.37,0:32:23.25,Main-207+,,0,0,0,,{\an8}Vay arkadaş, götüm yırtılacaktı.
Dialogue: 0,0:32:23.25,0:32:25.45,Main-207+,,0,0,0,,Yaşıyormuş lan, Trao!
Dialogue: 0,0:32:25.45,0:32:27.34,Main-207+,,0,0,0,,Öldüğünü ne zaman dedim ki?
Dialogue: 0,0:32:27.34,0:32:30.11,Main-207+,,0,0,0,,Tuvaletten anca gece çıkar demek istemiştim.
Dialogue: 0,0:32:30.11,0:32:31.92,Main-207+,,0,0,0,,Böyle mi anlatılır be!
Dialogue: 0,0:32:33.82,0:32:35.39,Main-207+,,0,0,0,,Kin-{\i1}sama{\i0}!
Dialogue: 0,0:32:35.51,0:32:36.48,Main-207+,,0,0,0,,Kiku!
Dialogue: 0,0:32:37.68,0:32:40.77,Main-207+,,0,0,0,,Dönmüşsün!
Dialogue: 0,0:32:41.47,0:32:42.94,Main-207+,,0,0,0,,Öyle özlemişim ki...
Dialogue: 0,0:32:42.94,0:32:44.81,Main-207+,,0,0,0,,elim ayağım boşaldı!
Dialogue: 0,0:32:44.81,0:32:46.27,Main-207+,,0,0,0,,Demek öyle...
Dialogue: 0,0:32:46.27,0:32:51.94,Main-207+,,0,0,0,,Hayret bir şeysin ya!\NMadem döndün, niye yanıma gelmedin?
Dialogue: 0,0:32:51.94,0:32:55.28,Main-207+,,0,0,0,,Nasıl endişelenmiştim!\NSoğuk davranmasana!
Dialogue: 0,0:32:55.28,0:32:56.22,Main-207+,,0,0,0,,Ü-Üzgünüm!
Dialogue: 0,0:32:56.22,0:32:57.99,Main-207+,,0,0,0,,Momonosuke-{\i1}sama{\i0} da güvende mi?
Dialogue: 0,0:32:57.99,0:32:58.98,Main-207+,,0,0,0,,Evet.
Dialogue: 0,0:32:58.98,0:33:01.45,Main-207+,,0,0,0,,Momo da hayatta demek?
Dialogue: 0,0:33:01.45,0:33:04.47,Main-207+,,0,0,0,,Bir saniye, Kiku ile Kin tanışıyor muydu?
Dialogue: 0,0:33:04.47,0:33:06.67,Main-207+,,0,0,0,,Bayağı da sıkı fıkılar.
Dialogue: 0,0:33:06.67,0:33:07.17,Main-207+,,0,0,0,,Ne?
Dialogue: 0,0:33:07.17,0:33:11.71,Main-207+,,0,0,0,,Kiku, Luffy-{\i1}donolar{\i0}'ın yanında ne işin var?
Dialogue: 0,0:33:11.71,0:33:15.01,Main-207+,,0,0,0,,Ne? Yoksa onlar...
Dialogue: 0,0:33:16.84,0:33:20.03,Main-207+,,0,0,0,,Kime, nereden anlatmaya başlasam ki?
Dialogue: 0,0:33:23.22,0:33:24.98,Main-207+,,0,0,0,,Evet, Kiku!
Dialogue: 0,0:33:24.98,0:33:28.73,Main-207+,,0,0,0,,Onlar, Orochi'nin hakkından gelmemize yardım edecek yaman dostlar!
Dialogue: 0,0:33:28.73,0:33:31.81,Main-207+,,0,0,0,,Luffy-{\i1}dono{\i0} ile Law-{\i1}dono{\i0}!
Dialogue: 0,0:33:31.81,0:33:33.36,Main-207+,,0,0,0,,Vay!
Dialogue: 0,0:33:33.36,0:33:35.82,Main-207+,,0,0,0,,Hey, Luffy!
Dialogue: 0,0:33:36.82,0:33:38.70,Main-207+,,0,0,0,,Sağ salim geldin demek!
Dialogue: 0,0:33:39.89,0:33:40.99,Main-207+,,0,0,0,,Momonosuke-{\i1}sama{\i0}?!
Dialogue: 0,0:33:40.99,0:33:45.07,Main-207+,,0,0,0,,Luffy, ben de kılıç ustalığımı geliştiriyordum!
Dialogue: 0,0:33:45.07,0:33:46.02,Main-207+,,0,0,0,,Harika!
Dialogue: 0,0:33:46.02,0:33:46.88,Main-207+,,0,0,0,,Bak şimdi!
Dialogue: 0,0:33:49.08,0:33:52.21,Main-207+,,0,0,0,,Momonosuke-{\i1}sama{\i0}!\Nİyi olmanıza çok sevindim!
Dialogue: 0,0:33:52.21,0:33:54.72,Main-207+,,0,0,0,,Teşekkürler Kiku, uzun zaman oldu.
Dialogue: 0,0:33:54.72,0:33:58.43,Main-207+,,0,0,0,,Köpek amca da Wano'ya geldi mi, Momo?
Dialogue: 0,0:33:58.43,0:33:59.64,Main-207+,,0,0,0,,Ne? Momo mu?
Dialogue: 0,0:33:59.64,0:34:03.59,Main-207+,,0,0,0,,Momonosuke-{\i1}sama{\i0}'ya öyle hitap etmek\Nsaygısızlık olur ama!
Dialogue: 0,0:34:04.02,0:34:06.02,Main-207+,,0,0,0,,Dert değil, Kiku.
Dialogue: 0,0:34:06.02,0:34:07.44,Main-207+,,0,0,0,,Ne? Peki öyleyse.
Dialogue: 0,0:34:07.44,0:34:10.77,Main-207+,,0,0,0,,Inuarashi de geldi tabii.
Dialogue: 0,0:34:10.77,0:34:12.73,Main-207+,,0,0,0,,Demek öyle.
Dialogue: 0,0:34:13.93,0:34:16.01,Main-207+,,0,0,0,,Geriye bir tek onlar kalıyor öyleyse.
Dialogue: 0,0:34:16.61,0:34:20.31,Main-207+,,0,0,0,,Daha varmadık mı ya?\NAyaklarıma kara sular indi.
Dialogue: 0,0:34:20.31,0:34:25.69,Main-207+,,0,0,0,,Kalenin kalıntılarına bakınmamızı\Nsöylediğinden bu dağda olmalılar.
Dialogue: 0,0:34:25.69,0:34:28.21,Main-207+,,0,0,0,,Neyse ki iletişime geçebildik ya!
Dialogue: 0,0:34:30.29,0:34:32.03,Main-207+,,0,0,0,,Hey!
Dialogue: 0,0:34:33.92,0:34:36.84,Main-207+,,0,0,0,,Gelmişsin, Luffy!
Dialogue: 0,0:34:36.84,0:34:38.34,Main-207+,,0,0,0,,– Luffy!\N– Luffy!
Dialogue: 0,0:34:38.34,0:34:40.76,Main-207+,,0,0,0,,Çocuklar! Gelin gelin!
Dialogue: 0,0:34:40.76,0:34:42.22,Main-207+,,0,0,0,,Luffy!
Dialogue: 0,0:34:42.22,0:34:44.47,Main-207+,,0,0,0,,İyi olmana çok sevindim!
Dialogue: 0,0:34:46.31,0:34:48.65,Main-207+,,0,0,0,,Garchu!
Dialogue: 0,0:34:48.65,0:34:52.36,Main-207+,,0,0,0,,Luffy! Samuray gibi giyinmişsin!\NYakıyorsun ha!
Dialogue: 0,0:34:52.36,0:34:53.98,Main-207+,,0,0,0,,Niye bu kadar geciktiniz?
Dialogue: 0,0:34:53.98,0:34:57.57,Main-207+,,0,0,0,,Deniz tabanına kadar\Nher yerde seni arıyorduk lan!
Dialogue: 0,0:34:57.57,0:35:01.82,Main-207+,,0,0,0,,Vivre kartını bulduğumuzdan\Nen azından hayatta olduğunu biliyorduk.
Dialogue: 0,0:35:01.82,0:35:03.41,Main-207+,,0,0,0,,Onami!
Dialogue: 0,0:35:03.41,0:35:04.78,Main-207+,,0,0,0,,Momo-{\i1}chan{\i0}!
Dialogue: 0,0:35:04.78,0:35:07.40,Main-207+,,0,0,0,,Kin'emon-{\i1}san{\i0}, sıhhatin yerinde mi?
Dialogue: 0,0:35:07.40,0:35:08.83,Main-207+,,0,0,0,,Öyle böyle.
Dialogue: 0,0:35:08.83,0:35:11.77,Main-207+,,0,0,0,,Momonosuke-{\i1}san{\i0}, seni de iyi gördüm.
Dialogue: 0,0:35:12.33,0:35:14.25,Main-207+,,0,0,0,,Düşündüğüm gibi, abaza çükütay!
Dialogue: 0,0:35:14.25,0:35:17.49,Main-207+,,0,0,0,,Tayfaya döndün demek Sanji-{\i1}dono{\i0}.
Dialogue: 0,0:35:17.49,0:35:19.77,Main-207+,,0,0,0,,Evet, endişelendirdiğim için üzgünüm.
Dialogue: 0,0:35:19.77,0:35:20.90,Main-207+,,0,0,0,,Kin-{\i1}san{\i0}!
Dialogue: 0,0:35:23.18,0:35:27.56,Main-207+,,0,0,0,,Bu insanlar da\NLuffytaro-{\i1}san{\i0}'ın yoldaşları mı oluyor?
Dialogue: 0,0:35:27.56,0:35:30.14,Main-207+,,0,0,0,,Ne güzel bir hanımefendi!
Dialogue: 0,0:35:31.98,0:35:33.27,Main-207+,,0,0,0,,Hey!
Dialogue: 0,0:35:33.28,0:35:35.45,Main-207+,,0,0,0,,Tamam lan! Çekil artık!
Dialogue: 0,0:35:35.45,0:35:37.28,Main-207+,,0,0,0,,Haddini aşıyorsun, Kemikichi!
Dialogue: 0,0:35:37.28,0:35:39.56,Main-207+,,0,0,0,,Karışmasana çocuğa, Brook!
Dialogue: 0,0:35:39.56,0:35:40.90,Main-207+,,0,0,0,,Değil mi, Momo-{\i1}chan{\i0}?
Dialogue: 0,0:35:40.90,0:35:43.30,Main-207+,,0,0,0,,Onami...
Dialogue: 0,0:35:41.87,0:35:43.30,Secondary-207+,,0,0,0,,Momo!
Dialogue: 0,0:35:43.70,0:35:45.86,Main-207+,,0,0,0,,Momo!
Dialogue: 0,0:35:45.86,0:35:48.83,Main-207+,,0,0,0,,Hepsi de Luffy-{\i1}dono{\i0}'nun yoldaşıdır!
Dialogue: 0,0:35:48.83,0:35:51.70,Main-207+,,0,0,0,,Bize arka çıkacak güvenilir insanlardır!
Dialogue: 0,0:35:56.87,0:36:00.81,Thoughts-207+,,0,0,0,,Momonosuke-{\i1}sama{\i0},\Nşükürler olsun ki yüzünüz gülüyor.
Dialogue: 0,0:36:02.63,0:36:04.39,Main-207+,,0,0,0,,Kaptan!
Dialogue: 0,0:36:04.39,0:36:06.18,Main-207+,,0,0,0,,Siz miydiniz?
Dialogue: 0,0:36:06.18,0:36:08.09,Main-207+,,0,0,0,,Bepo ne durumda?
Dialogue: 0,0:36:08.09,0:36:11.56,Main-207+,,0,0,0,,Cırcır olduğundan çalılıkta yatıp kalkıyor.
Dialogue: 0,0:36:12.78,0:36:14.69,Main-207+,,0,0,0,,Bir tane düzgün adam çıkmaz mı be?
Dialogue: 0,0:36:16.49,0:36:17.36,Main-207+,,0,0,0,,Hasır Şapka-{\i1}ya{\i0}.
Dialogue: 0,0:36:17.36,0:36:18.09,Main-207+,,0,0,0,,Ha?
Dialogue: 0,0:36:18.09,0:36:20.02,Main-207+,,0,0,0,,İçeride bir konuşalım hele.
Dialogue: 0,0:36:20.02,0:36:21.08,Main-207+,,0,0,0,,Tamam.
Dialogue: 0,0:36:21.08,0:36:24.33,Main-207+,,0,0,0,,Tarladan sağlıklı besinler çaldık.
Dialogue: 0,0:36:24.33,0:36:26.83,Main-207+,,0,0,0,,Hırsız sendin demek!
Dialogue: 0,0:36:29.20,0:36:32.62,Main-207+,,0,0,0,,Merkezimiz bu yıkık kale olacak.
Dialogue: 0,0:36:42.72,0:36:44.85,Main-207+,,0,0,0,,Ortalığı bok götürüyor!
Dialogue: 0,0:36:44.85,0:36:46.76,Main-207+,,0,0,0,,Kusura bakmayın arkadaşlar.
Dialogue: 0,0:36:54.06,0:36:56.94,Main-207+,,0,0,0,,Luffy-{\i1}dono{\i0}! Millet!
Dialogue: 0,0:36:56.94,0:36:59.86,Main-207+,,0,0,0,,Wano'ya geldiğiniz için teşekkür ederim!
Dialogue: 0,0:37:00.47,0:37:03.11,Main-207+,,0,0,0,,Tekrardan minnettarız!
Dialogue: 0,0:37:03.11,0:37:04.82,Main-207+,,0,0,0,,Lafı olmaz!
Dialogue: 0,0:37:04.82,0:37:06.07,Main-207+,,0,0,0,,Aynen!
Dialogue: 0,0:37:07.38,0:37:11.35,Main-207+,,0,0,0,,Öncelikle, Momonosuke-{\i1}sama{\i0}'nın\Nkimliğinin yanı sıra...
Dialogue: 0,0:37:11.35,0:37:15.92,Main-207+,,0,0,0,,sizinle paylaşmadığım bir konu daha var.
Dialogue: 0,0:37:17.13,0:37:19.38,Main-207+,,0,0,0,,Amma sırrınız varmış ha!
Dialogue: 0,0:37:19.38,0:37:22.88,Main-207+,,0,0,0,,Dökülün gitsin artık! O kadar mazimiz var!
Dialogue: 0,0:37:22.88,0:37:24.33,Main-207+,,0,0,0,,Harbiden!
Dialogue: 0,0:37:24.33,0:37:26.26,Main-207+,,0,0,0,,Arkadaşız artık!
Dialogue: 0,0:37:26.26,0:37:29.59,Main-207+,,0,0,0,,Ne deseniz haklısınız, özür dilerim!
Dialogue: 0,0:37:29.93,0:37:34.35,Main-207+,,0,0,0,,Fakat söyleyeceklerime inanmayacaksınız.
Dialogue: 0,0:37:35.47,0:37:37.23,Main-207+,,0,0,0,,Neymiş o? Yumurtla artık!
Dialogue: 0,0:37:41.94,0:37:45.96,Main-207+,,0,0,0,,Tanıştığınız Kanjuro ile Raizo'yla...
Dialogue: 0,0:37:45.96,0:37:48.82,Main-207+,,0,0,0,,buradaki üçümüz olmak üzere...
Dialogue: 0,0:37:48.82,0:37:50.66,Main-207+,,0,0,0,,beşimiz beraber...
Dialogue: 0,0:37:51.36,0:37:53.16,Main-207+,,0,0,0,,esasen...
Dialogue: 0,0:37:54.76,0:37:57.12,Main-207+,,0,0,0,,geçmişten geldik!
Dialogue: 0,0:37:57.89,0:37:58.46,Main-207+,,0,0,0,,Ha?
Dialogue: 0,0:37:58.46,0:37:59.68,Main-207+,,0,0,0,,– Ne?\N– Ne?
Dialogue: 0,0:37:59.68,0:38:01.34,Main-207+,,0,0,0,,Geçmişten mi?
